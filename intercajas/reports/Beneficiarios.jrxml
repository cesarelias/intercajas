	<parameter name="SolicitudID" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select a.id,
            b.nombres || ' ' || b.apellidos as nombres, 
             b.numero_documento,
         case a.parentesco
             when 0 then
              'Conyuge'
             when 1 then
              'Descendiente'
             when 2 then
              'Ascendiente'
             else
              'No Aplica'
             end as parentesco,
         case a.estado 
            when 0 then
               'Pendiente'
            when 1 then
               'Finalizado'
            end as estado,
          case a.tipo
             when 0 then
               'Titular'
             when 1 then
               'Derechohabiente'
              end as tipo
   from solicitud_beneficiario a, beneficiario b
  where a.beneficiario_id = b.id
     and a.solicitud_id = $P{SolicitudID}]]>
	</queryString>
