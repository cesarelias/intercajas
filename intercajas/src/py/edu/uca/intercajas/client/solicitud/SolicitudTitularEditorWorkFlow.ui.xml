<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gt="urn:import:py.edu.uca.intercajas.client.solicitud">
	<ui:style>
	
	</ui:style>
	<g:DockLayoutPanel unit="PX" width="720px" height="470px">
		<g:center>

			<g:DockLayoutPanel height="100%" width="100%">
				<g:north size="110">
					<g:HorizontalPanel>
						<gt:SolicitudTitularEditor ui:field="solicitudTitularEditor" />
						<g:HorizontalPanel>
							<g:Button ui:field="agregarAdjunto">Agregar Adjunto</g:Button>
							<g:ScrollPanel width="250px" height="100px">
								<g:FlexTable ui:field="adjuntos">
								</g:FlexTable>
							</g:ScrollPanel>
						</g:HorizontalPanel>
					</g:HorizontalPanel>
				</g:north>
				<g:center>
					<gt:TablaPeriodoAporteDeclarado
						ui:field="tablaPeriodoAporteDeclarado" />
				</g:center>
			</g:DockLayoutPanel>
		</g:center>
		<g:south size="40">
			<g:HorizontalPanel>
				<g:Button ui:field="cancelar">Cancelar</g:Button>
				<g:Button ui:field="guardar">Guardar</g:Button>
			</g:HorizontalPanel>
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder> 