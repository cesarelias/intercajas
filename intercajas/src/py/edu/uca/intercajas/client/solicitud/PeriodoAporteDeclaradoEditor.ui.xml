<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:e="urn:import:com.google.gwt.editor.ui.client"
	xmlns:dp="urn:import:com.google.gwt.user.datepicker.client">
	<ui:style>
	</ui:style>
	<g:HTMLPanel>
		<table>
			<tr>
				<td width="120px">Caja</td>
				<td>
					<g:ValueListBox ui:field="caja" />
				</td>
			</tr>
			<tr>
				<td>Inicio</td>
				<td>
					<dp:DateBox ui:field="inicio" />
				</td>
			</tr>
			<tr>
				<td>Fin</td>
				<td>
					<dp:DateBox ui:field="fin" />
				</td>
			</tr>
			<tr>
				<td>Lugar</td>
				<td>
					<g:SuggestBox ui:field="lugar" />
				</td>
			</tr>
			<tr>
			    <td>
			        
			    </td>
			    <td>
			        <g:HorizontalPanel>
			            <g:Button text="Cancelar" ui:field="cancel"/>
			            <g:Button text="Guardar" ui:field="save"/>
			        </g:HorizontalPanel>
			    </td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 