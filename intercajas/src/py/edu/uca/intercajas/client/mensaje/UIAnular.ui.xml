<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:dp="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:gt="urn:import:py.edu.uca.intercajas.client.mensaje">

	<ui:style>
		.important {
		font-weight: bold;
		}
	</ui:style>
	<g:DockLayoutPanel unit="PX" width="720px" height="470px">
		<g:center>
			<g:DockLayoutPanel height="100%" width="100%">
				<g:center>
					<g:VerticalPanel>
						<g:HTMLPanel>
							<table>
								<tr>
									<td width="120px">Obseracion</td>
									<td>
										<g:TextArea ui:field="observacion" />
									</td>
								</tr>
							</table>
						</g:HTMLPanel>
					</g:VerticalPanel>
				</g:center>
			</g:DockLayoutPanel>
		</g:center>
		<g:south size="40">
			<g:HorizontalPanel>
				<g:Button ui:field="cancelar">Cancelar</g:Button>
				<g:Button ui:field="anular">Anular</g:Button>
			</g:HorizontalPanel>
		</g:south>
	</g:DockLayoutPanel>

</ui:UiBinder> 