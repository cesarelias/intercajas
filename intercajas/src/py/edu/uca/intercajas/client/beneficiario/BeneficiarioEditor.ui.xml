<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:e="urn:import:com.google.gwt.editor.ui.client"
	xmlns:gt="urn:import:py.edu.uca.intercajas.client.beneficiario"
	xmlns:dp="urn:import:com.google.gwt.user.datepicker.client">
	<ui:style>
	.ui-datepicker {
		margin-left: 1px;
		z-index: 25;
	}
	</ui:style>
	<g:HTMLPanel>
		<table>
			<gt:DocumentoIdentidadEditor ui:field="documento" />
		</table>
		<table>
			<tr>
				<td width="120px">Nombre</td>
				<td>
					<e:ValueBoxEditorDecorator ui:field="nombres">
						<e:valuebox>
							<g:TextBox />
						</e:valuebox>
					</e:ValueBoxEditorDecorator>
				</td>
			</tr>
			<tr>
				<td>Apellidos</td>
				<td>
					<e:ValueBoxEditorDecorator ui:field="apellidos">
						<e:valuebox>
							<g:TextBox />
						</e:valuebox>
					</e:ValueBoxEditorDecorator>
				</td>
			</tr>
			<tr>
				<td>Sexo</td>
				<td>
					<gt:SexoEditor ui:field="sexo" />
				</td>
			</tr>
			<tr>
				<td>Fecha Nacimineto</td>
				<td>
					<dp:DateBox ui:field="fechaNacimiento" />
				</td>
			</tr>
		</table>
		<table>
			<gt:DireccionEditor ui:field="direccion" />
		</table>

	</g:HTMLPanel>

</ui:UiBinder> 
