<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:mail='urn:import:py.edu.uca.intercajas.client.menumail'>

  <g:DockLayoutPanel unit='EM'>
    <g:north size='5'>
      <mail:TopPanel ui:field='topPanel' />
    </g:north>

    <g:center>
      <g:SplitLayoutPanel>
        <g:west size='192'>
          <mail:Shortcuts ui:field='shortcuts' />
        </g:west>

        <g:center>
          <g:SimplePanel ui:field="main" />
        </g:center>
      </g:SplitLayoutPanel>
    </g:center>
  </g:DockLayoutPanel>


</ui:UiBinder>


